#!/bin/bash

echo "Executando alguns comandos"
./argus -e "ls -la"
sleep 0.1 
./argus -e "cat /etc/passwd | wc "
sleep 0.1
./argus -e "date"
sleep 0.1
./argus -e "pwd"
sleep 0.1 
./argus -e "grep -v Ë†# /etc/passwd | cut -f7 -d: | uniq | wc -l"
sleep 0.1 
./argus -e "tail -f /dev/null"
sleep 0.1 
./argus -m 10
sleep 0.1 
./argus -i 5
sleep 0.1 
./argus -e "tail -f /dev/null"
sleep 0.1 
./argus -e "tail -f /dev/null | wc"
sleep 0.1 
./argus -e "ls | cat | tail -f /dev/null"
sleep 0.1 
./argus -e "cat /etc/passwd | sleep 5 | ls -la"
sleep 0.1 
./argus -e "cat /etc/passwd | sleep 4 | ls -la"
sleep 0.1
./argus -m 0
sleep 0.1
./argus -i 0
sleep 0.1
./argus -e "tail -f /dev/null | cat | wc"
sleep 0.1 
./argus -e "ls | cat | tail -f /dev/null"